    "sanitize-html": "^2.13.1",
    "sharp": "^0.33.5",
    "sharp-ico": "^0.1.5",
    "sirv": "^3.0.0",
    "slash": "^5.1.0",
    "typescript": "^5.7.2",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "zod": "^3.23.8",
    "zod-form-data": "^2.0.2"
