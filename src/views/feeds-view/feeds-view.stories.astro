---
import type { ComponentProps } from "astro/types";
import { getStoryMeta } from "../../utils/stories";
import FeedsViewComponent from "./feeds-view.astro";

const title = "FeedsView";
const { breadcrumb, seo } = getStoryMeta({
  breadcrumb: {
    kind: "views",
    route: Astro.url.pathname,
    title,
  },
});

const fakePage: ComponentProps<typeof FeedsViewComponent>["entry"] = {
  Content: Fragment,
  collection: "pages",
  description: "The feeds page.",
  hasContent: false,
  headings: [],
  id: "en/feeds",
  locale: "en",
  meta: {
    publishedOn: new Date(),
    updatedOn: new Date(),
  },
  route: "/feeds",
  seo: {
    description: "",
    title,
  },
  slug: "feeds",
  title,
};
---

<FeedsViewComponent breadcrumb={breadcrumb} entry={fakePage} seo={seo} />
