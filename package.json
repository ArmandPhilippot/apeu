    "@types/nodemailer": "^6.4.17",
    "@types/sanitize-html": "^2.13.0",
    "@types/web-app-manifest": "^1.0.8",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^2.1.8",
    "cspell": "^8.16.1",
    "cypress": "^13.16.1",
    "eslint": "^9.16.0",
    "eslint-plugin-astro": "^1.3.1",
