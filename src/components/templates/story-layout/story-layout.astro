---
import type { StoryLayoutProps } from "../../../lib/astro/integrations/astro-stories";
import PageLayout from "../page-layout/page-layout.astro";

type Props = StoryLayoutProps;

const { story } = Astro.props;
---

<PageLayout
  breadcrumb={story.breadcrumb}
  seo={{ title: story.label }}
  title={story.label}
>
  <slot slot="body" />
</PageLayout>
