---
import type { ComponentProps } from "astro/types";
import { getStoryMeta } from "../../utils/stories";
import ContactViewComponent from "./contact-view.astro";

const title = "ContactView";
const { breadcrumb, seo } = getStoryMeta({
  breadcrumb: {
    kind: "views",
    route: Astro.url.pathname,
    title,
  },
});

const fakePage: ComponentProps<typeof ContactViewComponent>["entry"] = {
  Content: Fragment,
  collection: "pages",
  description: "The contact page.",
  hasContent: false,
  headings: [],
  id: "en/contact",
  locale: "en",
  meta: {
    publishedOn: new Date(),
    updatedOn: new Date(),
  },
  route: "/contact",
  seo: {
    description: "",
    title,
  },
  slug: "contact",
  title,
};
---

<ContactViewComponent breadcrumb={breadcrumb} entry={fakePage} seo={seo} />
